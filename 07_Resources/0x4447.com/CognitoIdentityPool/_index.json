{
	"CognitoIdentityPool": {
		"Type": "AWS::Cognito::IdentityPool",
		"Properties": {
			"IdentityPoolName": "zer0x",
			"AllowUnauthenticatedIdentities": false,
			"CognitoIdentityProviders": [
				{
					"ClientId": { "Ref": "CognitoUserPoolClients" },
					"ProviderName": { "Fn::GetAtt": ["CognitoUserPool", "ProviderName"] }
				}
			]
		}
	}
}
