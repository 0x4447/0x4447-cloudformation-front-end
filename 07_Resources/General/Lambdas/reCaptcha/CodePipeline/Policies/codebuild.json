{
	"CodePipelineRecaptchaCB": {
		"Type": "AWS::IAM::Policy",
		"Properties": {
			"PolicyName": "code_build",
			"Roles": [ { "Ref": "CodePipelineRecaptchaRole" } ],
			"PolicyDocument": {
				"Version": "2012-10-17",
				"Statement": [
					{
						"Action": [
							"codebuild:BatchGetBuilds",
							"codebuild:StartBuild"
						],
						"Resource": "*",
						"Effect": "Allow"
					}
				]
			}
		}
	}
}
